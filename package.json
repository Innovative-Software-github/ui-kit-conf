    "compile:clean": "rm -rf dist/*",
    "compile:code": "rollup --config rollup.config.js",
    "compile:types": "tsc --p tsconfig.types.json",
    "checkTypes": "tsc --p tsconfig.types.json",
    "pub-beta": "npm run build && npm version prerelease --preid=beta && npm publish --tag beta",
